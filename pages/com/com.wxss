/* pages/com/com.wxss */
form{
  width: 100%;
  background-color: #2f2f2f;

}
.hr{
  border: 2rpx solid #cccccc;
  width: 90%;
  margin: 0 auto;
  opacity: 0.3;
}
.item{
  margin: 0 auto;
  border: 2rpx solid #cccccc;
  width: 90%;
  height: 80rpx;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
.item input{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 35rpx;
  color: #999;

}
.iphoneUserInfo{
  margin: 0 auto;
  width: 90%;
  display: flex;
  flex-direction: row;
  margin-bottom: 30rpx;
}
.code{
  margin: 0 auto;
  border: 2rpx solid #ccc;
  width: 40%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx; 
  text-align: center;
}
.registerBtn{
  margin-bottom: 30rpx;
  

}